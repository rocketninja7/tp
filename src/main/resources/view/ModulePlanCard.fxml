<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" id="cardPane" fx:id="modulePlanCardPane" alignment="TOP_CENTER"
      xmlns="http://javafx.com/javafx/20.0.1">
    <children>
        <Label fx:id="semester" alignment="CENTER" contentDisplay="CENTER" styleClass="cell_big_label" text="\$semester"
               textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="14.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="10.0"/>
            </VBox.margin>
        </Label>
        <ListView fx:id="modules" prefHeight="200.0" prefWidth="170.0" VBox.vgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="2.0" right="2.0" top="20.0"/>
    </padding>
</VBox>
