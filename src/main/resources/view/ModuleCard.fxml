<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" fx:id="moduleCardPane" alignment="CENTER" maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefWidth="130.0"
      xmlns="http://javafx.com/javafx/20.0.1">
    <children>
        <Label fx:id="moduleCode" alignment="CENTER" minWidth="90.0" prefWidth="90.0" text="CS2030S"
               textAlignment="CENTER" textOverrun="CLIP" wrapText="true" HBox.hgrow="ALWAYS"/>
        <Label fx:id="grade" alignment="CENTER" contentDisplay="CENTER" maxWidth="30.0" minWidth="30.0" prefWidth="30.0"
               text="A+">
            <HBox.margin>
                <Insets left="5.0"/>
            </HBox.margin>
        </Label>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</HBox>
